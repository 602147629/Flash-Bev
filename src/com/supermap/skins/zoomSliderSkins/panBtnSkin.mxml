<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	
	<s:Rect left="-3" right="-3" top="-3" bottom="-3" radiusX="3" radiusY="3">
		<s:fill>
			<s:SolidColor color="0" alpha="0.7" alpha.over="0.5"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="0xffffff" />
		</s:stroke>
	</s:Rect> 
	
	<mx:Image source="assets/navigation/pan.png" verticalCenter="0" horizontalCenter="0"/>
	
</s:Skin>
